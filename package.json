{
  "name": "keys",
  "productName": "Keys",
  "version": "0.0.19",
  "description": "Key management, signing and encryption.",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config configs/webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config configs/webpack.renderer.prod.config.js",
    "build": "yarn run build-main && yarn run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config configs/webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config configs/webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 yarn run start-renderer-dev",
    "prestart": "yarn run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "yarn run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && yarn run resources && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "version": "echo $npm_package_version",
    "resources": "./scripts/resources.sh",
    "release": "yarn run build && yarn run resources && electron-builder --publish always",
    "publish": "./scripts/publish.sh"
  },
  "main": "./dist/main.js",
  "build": {
    "productName": "Keys",
    "appId": "pub.Keys",
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "extraResources": [
      "src/renderer/rpc/keys.proto",
      "bin/keysd",
      "bin/keys",
      "bin/updater"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "afterSign": "./scripts/notarize.js",
    "afterAllArtifactBuild": "./scripts/fix.js",
    "mac": {
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "entitlements": "resources/entitlements.mac.plist",
      "entitlementsInherit": "resources/entitlements.mac.plist",
      "publish": [
        "github"
      ],
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keys-pub/app.git"
  },
  "author": {
    "name": "Gabriel Handford",
    "email": "gabrielh@gmail.com",
    "url": "https://github.com/gabriel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keys-pub/app/issues"
  },
  "keywords": [],
  "homepage": "https://github.com/keys-pub/app#README",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "key-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "12.12.6",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.1.6",
    "@types/react-router": "^5.1.4",
    "@types/redux-logger": "^3.0.6",
    "@types/webpack-env": "^1.13.3",
    "babel-loader": "^8.0.2",
    "cross-env": "^6.0.0",
    "cross-spawn": "^7.0.0",
    "css-loader": "^3.2.0",
    "electron": "^8.0.1",
    "electron-builder": "^22.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.2.1",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.12.1",
    "react-hot-loader": "^4.3.6",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "tcomb": "^3.2.27",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^0.6.15",
    "@ln-zap/proto-loader": "^0.5.1",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "@material-ui/styles": "^4.3.0",
    "connected-react-router": "^6.0.0",
    "electron-updater": "^4.2.2",
    "electron-window-state": "^5.0.3",
    "getenv": "^1.0.0",
    "google-protobuf": "^3.5.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.9",
    "lodash": "^4.17.10",
    "node-emoji": "^1.10.0",
    "query-string": "^6.8.3",
    "react": "^16.8.0",
    "react-dom": "^16.5.1",
    "react-redux": "^7.0.0",
    "react-router": "^5.1.2",
    "react-virtualized": "^9.20.0",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.1.0",
    "slate": "^0.57.1",
    "slate-md-serializer": "^5.3.1",
    "slate-react": "^0.57.1",
    "source-map-support": "^0.5.6",
    "typeface-open-sans": "^0.0.75",
    "typeface-roboto": "^0.0.75",
    "typeface-roboto-mono": "^0.0.75"
  },
  "engines": {
    "node": ">=10.x <14",
    "yarn": ">=1.x"
  }
}
